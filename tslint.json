{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "only-arrow-functions": false,
    "unified-signatures": false,
    "quotemark": [true, "double"],
    "ordered-imports": false,
    "no-string-literal": false,
    "arrow-parens": false,
    "max-classes-per-file": false,
    "array-type": false
  }
}
